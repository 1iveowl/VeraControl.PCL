<?xml version="1.0"?>
<package >
  <metadata>
    <id>VeraControl.PCL</id>
    <version>0.3.3</version>
    <title>Vera Control - Xamarin PCL</title>
    <authors>Jasper H. Bojsen</authors>
    <owners>Jasper H. Bojsen</owners>
    <licenseUrl>https://github.com/1iveowl/VeraControl.PCL/blob/master/license</licenseUrl>
    <projectUrl>https://github.com/1iveowl/VeraControl.PCL</projectUrl>
    <iconUrl>https://nuget.org/Content/Images/packageDefaultIcon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>PCL for controlling Vera devices. Cross platofrom for iOS, Android and Windows 10 (UWP)</description>
    <releaseNotes>Implemented support for devices: BinaryLight, TemperatureSensor, Thermostat, VSwitch and VContainer</releaseNotes>
    <copyright>Copyright 2016</copyright>
    <tags>PCL, Xamarin, Vera, ios, android, windows 10, uwp</tags>

    <dependencies>

      <group targetFramework="monoandroid60">
        <dependency id="AutoMapper" version="5.1.1" />
        <dependency id="Newtonsoft.Json" version="9.0.1"/>
        <dependency id="modernhttpclient" version="2.4.2"/>
      </group>

      <group targetFramework="xamarinios10">
        <dependency id="AutoMapper" version="5.1.1" />
        <dependency id="Newtonsoft.Json" version="9.0.1"/>
        <dependency id="modernhttpclient" version="2.4.2"/>
      </group>

      <group targetFramework="portable45-net45+win8">
        <dependency id="AutoMapper" version="5.1.1" />
        <dependency id="Newtonsoft.Json" version="9.0.1"/>
      </group>

      <group targetFramework="uap10.0">
        <dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="5.2.2" />
        <dependency id="AutoMapper" version="5.1.1" />
        <dependency id="Newtonsoft.Json" version="9.0.1"/>
      </group>
    </dependencies>

  </metadata>
  <files>
  <!--Core-->
      <file src="Foundation\VeraControl.NET\bin\Release\VeraControl.dll"
          target="lib\portable45-net45+win8\VeraControl.dll" />
      <file src="Foundation\VeraControl.NET\bin\Release\VeraControl.pdb"
        target="lib\portable45-net45+win8\VeraControl.pdb" />
      <file src="Foundation\VeraControl.NET\bin\Release\IVeraControl.dll"
          target="lib\portable45-net45+win8\IVeraControl.dll" />
      <file src="Foundation\VeraControl.NET\bin\Release\IVeraControl.pdb"
        target="lib\portable45-net45+win8\IVeraControl.pdb" />

        <!--Xamarin.Android-->
      <file src="CrossPlatform\VeraControl.Android\bin\Release\VeraControl.dll"
          target="lib\monoandroid60\VeraControl.dll" />
      <file src="CrossPlatform\VeraControl.Android\bin\Release\VeraControl.pdb"
        target="lib\monoandroid60\VeraControl.pdb" />
      <file src="CrossPlatform\VeraControl.Android\bin\Release\IVeraControl.dll"
          target="lib\monoandroid60\IVeraControl.dll" />
      <file src="CrossPlatform\VeraControl.Android\bin\Release\IVeraControl.pdb"
        target="lib\monoandroid60\IVeraControl.pdb" />

        <!--Xamarin.iOS Unified-->
      <file src="CrossPlatform\VeraControl.iOS\bin\Release\VeraControl.dll"
          target="lib\xamarinios10\VeraControl.dll" />
      <file src="CrossPlatform\VeraControl.iOS\bin\Release\VeraControl.pdb"
        target="lib\xamarinios10\VeraControl.pdb" />
      <file src="CrossPlatform\VeraControl.iOS\bin\Release\IVeraControl.dll"
          target="lib\xamarinios10\IVeraControl.dll" />
      <file src="CrossPlatform\VeraControl.iOS\bin\Release\IVeraControl.pdb"
        target="lib\xamarinios10\IVeraControl.pdb" />

        <!--Xamarin.iOS Unified-->
      <file src="CrossPlatform\VeraControl.UWP\bin\Release\VeraControl.dll"
          target="lib\uap10.0\VeraControl.dll" />
      <file src="CrossPlatform\VeraControl.UWP\bin\Release\VeraControl.pdb"
        target="lib\uap10.0\VeraControl.pdb" />
      <file src="CrossPlatform\VeraControl.UWP\bin\Release\IVeraControl.dll"
          target="lib\uap10.0\IVeraControl.dll" />
      <file src="CrossPlatform\VeraControl.UWP\bin\Release\IVeraControl.pdb"
        target="lib\uap10.0\IVeraControl.pdb" />

  </files>
</package>